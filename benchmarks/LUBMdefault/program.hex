pcinemployee("Employee",X):-inemployee(X).
mcinemployee("Employee",X):-innegemployee(X).


inemployee(X):-&cDL[OWLONTOLOGY,pcinemployee,mcinemployee,c,d,"ResearchAssistant"](X),
		not &cDL[OWLONTOLOGY,pcinemployee,mcinemployee,c,d,"-Employee"](X), domain(X).

innegemployee(X):-&cDL[OWLONTOLOGY,pcinemployee,mcinemployee,c,d,"Student"](X),
		not &cDL[OWLONTOLOGY,pcinemployee,mcinemployee,c,d,"Employee"](X), domain(X).

