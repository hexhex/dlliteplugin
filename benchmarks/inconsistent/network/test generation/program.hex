
route(X,Y):-open(X), open(Y), &rDL[OWLONTOLOGY,a,b,c,d,"edge"](X,Y).

go(X,Y):-route(X,Y), not &rDL[OWLONTOLOGY,pc,b,c,d,"forbid"](X,Y).

open(X) v block(X):-not &cDL[OWLONTOLOGY,a,b,c,d,"-Avail"](X), &cDL[OWLONTOLOGY,a,b,c,d,"Node"](X), domain(X).

negis(X):-domain(X), go(X,Y), X!=Y.

:-node(X), not negis(X).

pc("Block",X):-block(X).
