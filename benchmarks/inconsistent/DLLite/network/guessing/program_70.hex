route(X,Z):-route(X,Y),route(Y,Z).

go(X,Y):-open(X), open(Y), edge(X,Y).

route(X,Y):-go(X,Y), not &rDL[OWLONTOLOGY,pc,b,c,d,"forbid"](X,Y).

open(X) v block(X):-not &cDL[OWLONTOLOGY,a,b,c,d,"-Avail"](X), domain(X).

open(X):-node(X), not &cDL[OWLONTOLOGY,a,b,c,d,"Broken"](X), not block(X).

negis(X):-domain(X), route(X,Y), X!=Y.

:-domain(X), not negis(X).

pc("Block",X):-block(X).
